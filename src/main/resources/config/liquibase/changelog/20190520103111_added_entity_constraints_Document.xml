<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Document.
    -->
    <changeSet id="20190520103111-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="content_id"
                                 baseTableName="document"
                                 constraintName="fk_document_content_id"
                                 referencedColumnNames="id"
                                 referencedTableName="content"/>

        <addForeignKeyConstraint baseColumnNames="insurance_form_id"
                                 baseTableName="document"
                                 constraintName="fk_document_insurance_form_id"
                                 referencedColumnNames="id"
                                 referencedTableName="insurance_form"/>

    </changeSet>
</databaseChangeLog>
